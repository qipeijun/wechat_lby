/* pages/team-travel/team-travel.wxss */

.banner-wrap{
  width: 100%;
  height: 431rpx;
  position: relative;
}

.banner{
  width: 100%;
  height: 431rpx;
}
.banner swiper-item{
  background-color: lightblue
}
.banner swiper-item:nth-child(2n){
  background-color: lightgreen
}


.nav-wrap{
  width: 100%;
  height: 407rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.nav-list{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
}

.nav-item{
  width: 25%;
  height: 186rpx;
}
.nav-item image{
  width: 100%;
  height: 130rpx;
  margin-bottom: 14rpx;
}
.nav-item text{
  display: block;
  text-align: center;
  font-size: 26rpx;
  color: #2c2727;
}















